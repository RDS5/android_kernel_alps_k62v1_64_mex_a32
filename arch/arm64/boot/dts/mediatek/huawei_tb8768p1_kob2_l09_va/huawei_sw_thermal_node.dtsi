/* shell thermal config and algorithm AR000AFR67 */
&hisi_shell {
	compatible = "hisi,shell-temp";
	interval = <5000>;
	bias = <500912>;
	count = <7>;
	tsensor_para = "10000","100000","0";
	ntc_para = "2000","80000","-20000";
	status = "ok";
	sensors {
		sensor0 {
			type = "msm_therm";
			coef = "1215","655","198","468","2617","1959","16398";
		};
		sensor1 {
			type = "pa_therm0";
			coef = "4045","492","813","231","-895","-1911","-17654";
		};
		sensor2 {
			type = "battery";
			coef = "857000","6407000","15901000","17728000","24706000","26262000","51406000";
		};
		sensor3 {
			type = "COOL_THERM";
			coef = "-15155","-8338","-9332","-10193","-9578","-9750","-17546";
		};
	};
};

